{
  "name": "nebari-docs",
  "version": "0.0.1",
  "description": "The main documentation website for Nebari",
  "private": true,
  "author": {
    "name": "Nebari development team",
    "url": "https://nebari.dev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nebari-dev/nebari/"
  },
  "bugs": {
    "url": "https://github.com/nebari-dev/nebari/issues"
  },
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "lint": "eslint \"*.{js,jsx,css,scss}\"",
    "lint:fix": "yarn run lint -- --fix",
    "format": "prettier --check \"**/*.{md, mdx, json, toml}\"",
    "format:fix": "prettier --write \"**/*.{md, mdx, json, toml}\"",
    "prepare": "cd .. && husky install docs/.husky",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "**/*.{js,jsx,css}": [
      "yarn run lint:fix"
    ],
    "**/*.{json,md,mdx,toml}": [
      "yarn run format:fix"
    ]
  },
  "dependencies": {
    "@docusaurus/core": "3.5.0",
    "@docusaurus/preset-classic": "3.5.0",
    "@docusaurus/theme-mermaid": "3.5.0",
    "@mdx-js/react": "^3.0.0",
    "clsx": "^2.1.1",
    "docusaurus-lunr-search": "^3.3.0",
    "docusaurus-plugin-sass": "^0.2.5",
    "prism-react-renderer": "^2.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.77.8"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.25.1",
    "@docusaurus/module-type-aliases": "3.5.0",
    "@eslint/compat": "^1.1.1",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.9.0",
    "eslint": "^9.9.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "globals": "^15.9.0",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.8",
    "netlify-plugin-cache": "^1.0.3",
    "prettier": "^3.3.3"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0"
  }
}
